{
	"version": "3.7",
	"extends": "../tsconfig-base.json",
	"compilerOptions": {
		"baseUrl": ".",
		"rootDir": "src",
		"outDir": "./dist",
		"sourceMap": true,
		"target": "es2015",
		"module": "CommonJS",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"./src/**/*.ts"
	],
	"exclude": [
		"webpack.config.ts"
	]
}
