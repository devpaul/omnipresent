{
	"services": [
		{
			"name": "log",
			"level": "debug"
		},
		{
			"name": "omnipresent"
		},
		{
			"name": "upload",
			"path": "/upload/*",
			"directory": "./uploads",
			"allowOverwrite": true,
			"createUploadDirectory": true
		},
		{
			"name": "file",
			"description": "presentation client",
			"basePath": "..",
			"showDirectoryContents": true,
			"paths": {
				"/producer/*": "./producer/output/dist",
				"/present-core/*": "./core/dist/esm",
				"/samples/*": "./server/samples",
				"/files/*": "./server/uploads",
				"/client/*": "./client/output/dev",
				"/slides/*": "./slides",
				"/present/*": "./present/dist",
				"*": "./client/output/dev"
			}
		}
	]
}
