{
	"services": [
		{
			"name": "omnipresent"
		},
		{
			"name": "upload",
			"path": "/upload/*",
			"directory": "./uploads",
			"allowOverwrite": true,
			"createUploadDirectory": true
		},
		{
			"name": "file",
			"description": "presentation client",
			"basePath": "..",
			"showDirectoryContents": true,
			"paths": {
				"/producer/*": "./producer/output/dist",
				"/core/*": "./core/dist/esm",
				"/client/*": "./client/output/dist",
				"/slides/*": "./slides",
				"*": "./client/output/dist"
			}
		}
	]
}
